<template name="medicine-view">
    <view class="flex-row"
          style="justify-content: space-between;width:650rpx;height:170rpx;border-radius:32rpx;background-color: antiquewhite;margin: 10rpx 0;padding: 0 37rpx 0 25rpx"
          bindtap="clickPhoto">
        <view class="flex-row">
            <view style="position: relative">
                <view style="width:90rpx;height:50rpx;background-color:black;border-radius:12rpx;opacity:0.2"></view>
                <view style="width:90rpx;height:50rpx;position: absolute;top: 0;left: 0;line-height: 50rpx;color: white;font-size: 26rpx;text-align: center;justify-content:center">1号仓</view>
            </view>
            <view style="margin-left: 20rpx;font-size: 38rpx;color: white;">复方降压片</view>
        </view>
        <view class="flex-row" style="color: white">
            <view style="width: 70rpx;height: 70rpx;background-color: white"></view>
            <view style="align-items: flex-end;display: flex">
                <view style="font-size: 40rpx;margin-left: 24rpx">X</view>
                <view style="font-size: 100rpx;margin-left: 5rpx;line-height: 100rpx">4</view>
            </view>
        </view>
    </view>
</template>